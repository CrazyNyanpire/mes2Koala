<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" %>
<form class="form-horizontal">
    <div class="form-group">
        <label class="col-lg-3 control-label">客户产品型号:</label>
        <div class="col-lg-9">
            <p class="form-control-static" id="customerProductNumberID"></p>
        </div>
    </div>
    <div class="form-group">
        <label class="col-lg-3 control-label">客户产品版本:</label>
        <div class="col-lg-9">
            <p class="form-control-static" id="customerProductRevisionID"></p>
        </div>
    </div>
    <div class="form-group">
        <label class="col-lg-3 control-label">所属直接客户:</label>
        <div class="col-lg-9">
            <p class="form-control-static" id="customerDirectDTOID"></p>
        </div>
    </div>
    <div class="form-group">
        <label class="col-lg-3 control-label">所属间接客户:</label>
        <div class="col-lg-9">
            <p class="form-control-static" id="customerIndirectDTOID"></p>
        </div>
    </div>
    <div class="form-group">
        <label class="col-lg-3 control-label">出货产品型号:</label>
        <div class="col-lg-9">
            <p class="form-control-static" id="shipmentProductNumberID"></p>
        </div>
    </div>
    <div class="form-group">
        <label class="col-lg-3 control-label">晶圆尺寸:</label>
        <div class="col-lg-9">
            <p class="form-control-static" id="">waferSizeID</p>
        </div>
    </div>
    <div class="form-group">
        <label class="col-lg-3 control-label">GROSS DIE:</label>
        <div class="col-lg-9">
            <p class="form-control-static" id="grossDieID"></p>
        </div>
    </div>
    <div class="form-group">
        <label class="col-lg-3 control-label">最低PASS报警:</label>
        <div class="col-lg-9">
            <p class="form-control-static" id="warningQtyID"></p>
        </div>
    </div>
    <div class="form-group">
        <label class="col-lg-3 control-label">报警指标分类:</label>
        <div class="col-lg-9">
            <p class="form-control-static" id="warningTypeID"></p>
        </div>
    </div>
    <div class="form-group">
        <label class="col-lg-3 control-label">测试时间/片:</label>
        <div class="col-lg-9">
            <p class="form-control-static" id="testTimeID"></p>
        </div>
    </div>
    <div class="form-group">
        <label class="col-lg-3 control-label">产品制程要求/片:</label>
        <div class="col-lg-9">
        </div>
    </div>
    <div class="form-group">
        <label class="col-lg-3 control-label">每片接触次数:</label>
        <div class="col-lg-9">
            <p class="form-control-static" id="touchQtyID"></p>
        </div>
    </div>
</form>