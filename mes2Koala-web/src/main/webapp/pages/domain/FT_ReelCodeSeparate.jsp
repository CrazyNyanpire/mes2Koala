        <label class="col-lg-3 control-label">Reel Code:</label>
        <div class="col-lg-6">
            <input style="display:inline; width:94%;" class="form-control"  type="text"  id="reelcode" disabled />
        </div>
    </div>
    <div class="form-group">
        <label class="col-lg-3 control-label">Quality:</label>
        <div class="col-lg-6">
            <input  style="display:inline; width:94%;" class="form-control"  type="text"  id="quality" disabled />
